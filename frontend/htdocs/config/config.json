		"apiUsersAdd": "/api/qgis-ui/users/add",
		"apiUsersDelete": "/api/qgis-ui/users/delete",
		"apiRoles": "/api/qgis-ui/roles",
		"apiRolesUuid": "/api/qgis-ui/roles/uuid/{uuid}",
		"apiScales": "/api/qgis-ui/scales",
